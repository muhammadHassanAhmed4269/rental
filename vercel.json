{
  "version": 2,
  "builds": [
    {
      "src": "client/src/index.js",
      "use": "@vercel/static-build",
      "config": {}
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/client/(.*)",
      "dest": "client/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ]
}
